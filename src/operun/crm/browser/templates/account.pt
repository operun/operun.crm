<html xmlns="http://www.w3.org/1999/xhtml"
      xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="operun.crm">
<body>

<metal:content-core fill-slot="content-core">
    <metal:content-core define-macro="content-core">

      <div tal:content="context/type"></div>
      <div tal:content="context/logo/filename"></div>
      <div tal:content="context/phone"></div>
      <div tal:content="context/fax"></div>
      <div tal:content="context/billing_email"></div>
      <div tal:content="context/website"></div>
      <div tal:content="context/address"></div>
      <div tal:content="context/ceo"></div>
      <div tal:content="context/invoice"></div>
      <div tal:content="context/zip"></div>
      <div tal:content="context/city"></div>
      <div tal:content="structure context/text/output"></div>

    </metal:content-core>
</metal:content-core>

</body>
</html>
