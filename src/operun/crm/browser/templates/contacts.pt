<html xmlns="http://www.w3.org/1999/xhtml"
      xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="operun.crm">
  <body>

    <metal:content-core fill-slot="content-core">
      <metal:content-core define-macro="content-core">

        <h2>Contact Listing</h2>

        <div id="contacts-container">
          <div class="contacts-wrapper" tal:define="contacts python:view.get_contacts()" tal:repeat="contact contacts">
            <div class="contact-wrapper" tal:define="obj python:contact.getObject()">
              <a href="" tal:attributes="href obj/absolute_url">
                <h3 tal:content="obj/title"></h3>
              </a>
            </div>
          </div>
        </div>

      </metal:content-core>
    </metal:content-core>

  </body>
</html>
